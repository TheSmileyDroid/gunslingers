[sub_resource type="Resource" id="Resource_h2777"]
script = ExtResource("1_obhhc")
character_name = ""
text = "(O grupo começa a colocar explosivos ao redor do minério brilhante. O Funcionário da Mineradora observa em silêncio, como se ainda tentasse entender tudo o que aconteceu)"

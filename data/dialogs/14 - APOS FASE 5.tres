[sub_resource type="Resource" id="Resource_0olng"]
script = ExtResource("1_obhhc")
character_name = ""
text = "(A caverna está em silêncio. O “rato-primordial” jaz morto no centro do salão subterrâneo. Ao redor, centenas de ratos cambaleiam, desnorteados, sem saber o que fazer)"
